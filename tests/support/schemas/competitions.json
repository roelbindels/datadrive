  "properties": {
    "data": {
      "$id": "/properties/data",
      "type": "array",
      "items": {
        "$id": "/properties/data/items",
        "type": "object",
        "properties": {
          "CompetitionId": {
            "$id": "/properties/data/items/properties/CompetitionId",
            "type": "integer",
            "title": "The Competitionid Schema ",
            "default": 0,
            "examples": [
              51469
            ]
          },
          "StartDate": {
            "$id": "/properties/data/items/properties/StartDate",
            "type": "string",
            "title": "The Startdate Schema ",
            "default": "",
            "examples": [
              "/Date(1527026400000)/"
            ]
          },
          "EndDate": {
            "$id": "/properties/data/items/properties/EndDate",
            "type": "string",
            "title": "The Enddate Schema ",
            "default": "",
            "examples": [
              "/Date(1527372000000)/"
            ]
          },
          "CompetitionName": {
            "$id": "/properties/data/items/properties/CompetitionName",
            "type": "string",
            "title": "The Competitionname Schema ",
            "default": "",
            "examples": [
              "Baloise Belgium Tour"
            ]
          },
          "CountryName": {
            "$id": "/properties/data/items/properties/CountryName",
            "type": "string",
            "title": "The Countryname Schema ",
            "default": "",
            "examples": [
              "BELGIUM"
            ]
          },
          "IsInProgress": {
            "$id": "/properties/data/items/properties/IsInProgress",
            "type": "boolean",
            "title": "The Isinprogress Schema ",
            "default": false,
            "examples": [
              true
            ]
          },
          "IsDone": {
            "$id": "/properties/data/items/properties/IsDone",
