          "FlagCode": {
            "$id": "/properties/data/items/properties/FlagCode",
            "type": "string",
            "title": "The Flagcode Schema ",
            "default": "",
            "examples": [
              "be"
            ]
          },
          "Date": {
            "$id": "/properties/data/items/properties/Date",
            "type": "string",
            "title": "The Date Schema ",
            "default": "",
            "examples": [
              "23 May-27 May 2018"
            ]
          }
        }
      }
    },
    "total": {
      "$id": "/properties/total",
      "type": "integer",
      "title": "The Total Schema ",
      "default": 0,
      "examples": [
        272
      ]
    }
  }
