          "CountryIsoCode2": {
            "$id": "/properties/data/items/properties/CountryIsoCode2",
            "type": "string",
            "title": "The Countryisocode2 Schema ",
            "default": "",
            "examples": [
              "BE"
            ]
          },
          "ClassCode": {
            "$id": "/properties/data/items/properties/ClassCode",
